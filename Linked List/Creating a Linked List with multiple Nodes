//Creating a linked list with five Dynamically created Nodes.


#include <iostream>
using namespace std;

class Node{
  public:
    int data;
    Node *next;
    Node(int data){
      this->data = data;
      next = NULL;
    }
    void print(){
      
    }
};

int main(){
  Node *n1 = new Node(22);
  Node *head = n1;
  Node *n2 = new Node(24);
  n1->next = n2;
  Node *n3 = new Node(26);
  n2->next = n3;
  Node *n4 = new Node(28);
  n3->next = n4;
  Node *n5 = new Node(30);
  n4->next = n5;
}
