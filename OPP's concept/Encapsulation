#include <iostream>
using namespace std;

class Student{
  private:
    int age;
    string name;
    int height;
  public:
    int getAge(){
      return age;
    }
    void setAge(int age){
      this->age = age;
    }
  
};
int main() 
{
   // Static Allocation
    Student *s1 = new Student();
    s1->setAge(12);
    cout<<s1->getAge();
    
}
